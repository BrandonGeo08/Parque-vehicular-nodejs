                    {{!-- Datos --}}
                    <div>
                        <form>
                            <div class="form-row">
                                <div class="col">
                                    <label><b>Fecha:</b></label>
                                    <input disabled class="form-control form-control-sm" type="date"
                                        value="{{fecha}}"><br>
                                    <label><b>Nombre empleado:</b></label>
                                    <input disabled class="form-control form-control-sm" type="text"
                                        value="{{nombreEmpleado}}">
                                </div>
                                <div class="col">
                                    <label><b>Hora:</b></label>
                                    <input disabled class="form-control form-control-sm" type="time"
                                        value="{{hora}}"><br>
