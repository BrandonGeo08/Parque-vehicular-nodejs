                                    <td>{{estadoBujiasBueno}}</td>
                                    <td>{{estadoBalatasDelBueno}}</td>
                                    <td>{{estadoBalatasTraBueno}}</td>
                                    <td>{{estadoFiltroAireBueno}}</td>
