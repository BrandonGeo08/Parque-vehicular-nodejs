{{!-- <!-- Estilo logo --> --}}
<style>
	h2{
    font-weight: 600;
    font-size: 1.1rem;
	color: #fff;
	}
	.toggle{
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-top: 1.4rem;
	}
	.toggle .logo{
    display: flex;
    gap: 0.5rem;
	}
	.toggle .logo img{
    width: 2rem;
    height: 2rem;
	}
</style>
{{!-- <!-- Final estilo logo --> --}}


<div class="main-header">
	<!-- Logo Header -->
	<div class="logo-header" data-background-color="dark2" >
		<div class="toggle">
			<a href="./datostotal.html" class="logo">
				<img src="/img/logo_borde.png">
				<h2>MI <span style="color: #09D368;">ABUELITO</span> TE PRESTA</h2>
			</a>
		</div>
		<button class="navbar-toggler sidenav-toggler ml-auto" type="button" data-toggle="collapse" data-target="collapse" aria-expanded="false" aria-label="Toggle navigation">
			<span class="navbar-toggler-icon">
				<i class="icon-menu"></i>
			</span>
		</button>
		<button class="topbar-toggler more">
			<i class="icon-options-vertical"></i>
		</button>
		<div class="nav-toggle">
			<button class="btn btn-toggle toggle-sidebar">
				<i class="icon-menu"></i>
			</button>
		</div>
	</div>
	<!-- End Logo Header -->

	<!-- Navbar Header -->
	<nav class="navbar navbar-header navbar-expand-lg" data-background-color="dark">
		<div class="container-fluid">
			<ul class="navbar-nav topbar-nav ml-md-auto align-items-center">
				
				<li class="nav-item dropdown hidden-caret">
					<a class="dropdown-toggle profile-pic" data-toggle="dropdown" href="#" aria-expanded="false">
						<div class="avatar-sm">
							<img src="/img/logo.png" alt="..." class="avatar-img rounded-circle">
						</div>
					</a>
					<ul class="dropdown-menu dropdown-user animated fadeIn">
						<div class="dropdown-user-scroll scrollbar-outer">
							{{#if user}}
							<li>
								<div class="user-box">
									<div class="avatar-lg">
										<img src="/img/logo.png" alt="image profile" class="avatar-img rounded">
									</div>
									<div class="u-text">
										<h4>{{user.clvEmployee}} {{user.fullname}}</h4>
										<p class="text-muted">{{user.username}}</p>
									</div>
								</div>
							</li>
							
							<li>
								<div class="dropdown-divider"></div>
								<a class="dropdown-item" href="/logout">Cerrar sesión</a>
							</li>
						</div>
					</ul>
				</li>
				{{else}}
				<li>
					<div class="user-box">
						<div class="u-text">
							<h4>Ningún usuario esta logueado</h4>
						</div>
					</div>
				</li>
				{{/if}}
			</ul>
		</div>
	</nav>
	<!-- End Navbar -->
</div>
	<!-- Sidebar -->
	<div class="sidebar sidebar-style-2" data-background-color="dark2">
		<div class="sidebar-wrapper scrollbar scrollbar-inner">
			<div class="sidebar-content">
				<div class="user">
					<div class="info">
						{{#if user}}
						<a data-toggle="collapse" href="#collapseExample" aria-expanded="true">
							<span>
								{{user.clvEmployee}} {{user.fullname}}
								<span class="user-level">{{user.username}}</span>
								<span class="caret"></span>
							</span>
						</a>
						<div class="clearfix"></div>

						<div class="collapse in" id="collapseExample">
							<ul class="nav">
								<li>
									<a href="/logout">
										<span class="link-collapse">Cerrar sesión</span>
									</a>
								</li>
							</ul>
						</div>
						{{else}}
						<a data-toggle="collapse" href="#collapseExample" aria-expanded="true">
							<span>
								Ningún usuario esta logueado
							</span>
						</a>
						{{/if}}
					</div>
				</div>
				<ul class="nav nav-primary">
					{{#if user}}
					<li class="nav-item">
						<a href="../data/relacionvehiculos">
							<i class="fas fa-home"></i>
							<span class="sub-item">Inicio</span>
						</a>
					</li>
					<li class="nav-item">
						<a href="../data/relacionvehiculos">
							<i class="fas fa-table"></i>
							<p>Parque Vehicular</p>
						</a>
					</li>
					<li class="nav-item">
						<a href="../data/tarjetadatos">
							<i class="fas fa-address-card"></i>
							<span class="sub-item">Tarjeta de datos</span>
						</a>
					</li>
					<li class="nav-item">
						<a href="../data/checklist">
							<i class="fas fa-check-square-o"></i>
							<span class="sub-item">Checklist vehiculos</span>
						</a>
					</li>
					{{/if}}
				</ul>
			</div>
		</div>
	</div>
<!-- End Sidebar -->






